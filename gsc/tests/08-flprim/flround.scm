(declare (extended-bindings) (not constant-fold) (not safe))

(define (test a b)
  (let ((x (##flround a)))
    (println (##fl= x b))))

(test  1.4  1.0)
(test  1.5  2.0)
(test  1.6  2.0)
(test  2.4  2.0)
(test  2.5  2.0)
(test  2.6  3.0)
(test -3.4 -3.0)
(test -3.5 -4.0)
(test -3.6 -4.0)
(test -4.4 -4.0)
(test -4.5 -4.0)
(test -4.6 -5.0)

(test  2.2  9.1)
